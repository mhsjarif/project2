<h1 class="cat-name"><%= @category.genre %> Attractions</h1>
  <div class="row attraction">
    <% @attractions.each do |attraction| %>
    <div class="col s6 m4">
      <div class="card">
        <div class="card-action">
          <h6 class="truncate cat-card-title"><a href="#" class="truncate"><%= link_to(attraction.name, attraction_path(attraction), :data => { turbolinks: false }) %></a>
          </h6>
        </div>
        <div class="card-image">
          <img src="<%= attraction.ride_image %>">
        </div>
      </div>
    </div>
  <% end %>
</div>

